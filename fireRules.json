{
  "rules": {
    ".read": "true",
    ".write": "true",
    "contacts": {
            "$contact": {
              "business_number": {
                ".validate": "newData.isString() && newData.val.matched([0-9]) && newData.val().legth < 10"
              },
                "name": {
                    ".validate": "newData.isString() && newData.val().length < 49 && newData.val().length > 1"
                },
                "primary": {
                    ".validate": "newData.isString() && newData.val().matches(['Fisher', 'Distributer', 'Processor', 'Fish Monger'])"
                },
                "address": {
                  ".validate": "newData.val().length < 50"
                },
                "province": {
                  ".validate": "newData.val().matches(['AB', 'BC', 'MB', 'NB', 'NL', 'NS', 'NT', 'NU', 'ON', 'PE', 'QC', 'SK', 'YT', “ “])"
                }

            }
        }
  }
}
